<AnchorPane fx:id="rootPane" prefHeight="593.0" prefWidth="960.0" style="-fx-background-color: transparent;"
            styleClass="shadow" stylesheets="@../CSSs/button.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codefathers.cfkclient.controllers.CreateCompany">
   <Pane prefHeight="593.0" prefWidth="593.0" style="-fx-background-color: #5fcd81;" styleClass="roudPaneLeft">
      <stylesheets>
         <URL value="@../CSSs/button.css"/>
         <URL value="@../CSSs/gradient.css"/>
      </stylesheets>
      <JFXButton fx:id="back" contentDisplay="CENTER" ellipsisString="" focusTraversable="false" layoutX="14.0"
                 layoutY="14.0" prefHeight="11.0" prefWidth="42.0" styleClass="mainPageMenuButtons" text=" ">
         <graphic>
            <FontIcon iconColor="WHITE" iconLiteral="fa-arrow-circle-left" iconSize="30"/>
         </graphic>
         <ripplerFill>
            <RadialGradient centerX="0.4822222391764323" centerY="0.5" cycleMethod="REFLECT" focusAngle="-31.35"
                            radius="0.7897196172553802">
               <stops>
                  <Stop color="#ccff00"/>
                  <Stop color="#32c2aa" offset="0.49794520547945204"/>
                  <Stop color="#ff4949" offset="1.0"/>
               </stops>
            </RadialGradient>
         </ripplerFill>
      </JFXButton>
      <ImageView fitHeight="451.0" fitWidth="465.0" layoutX="71.0" layoutY="14.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@../Images/company.png"/>
      </ImageView>
      <Label layoutX="54.0" layoutY="465.0" prefHeight="77.0" prefWidth="486.0" text="Create A Company"
             textFill="WHITE">
         <font>
            <Font name="Microsoft YaHei Light" size="54.0"/>
         </font>
      </Label>
   </Pane>
   <Pane layoutX="594.0" prefHeight="593.0" prefWidth="367.0" style="-fx-background-color: white;"
         styleClass="roudPaneRight" stylesheets="@../CSSs/button.css">
      <JFXTextField fx:id="name" labelFloat="true" layoutX="64.0" layoutY="77.0" prefHeight="38.0" prefWidth="240.0"
                    promptText="Company Name"/>
      <JFXTextField fx:id="category" labelFloat="true" layoutX="64.0" layoutY="161.0" prefHeight="38.0"
                    prefWidth="240.0" promptText="Company Category"/>
      <JFXTextField fx:id="phone" labelFloat="true" layoutX="64.0" layoutY="247.0" prefHeight="38.0"
                    prefWidth="240.0" promptText="Phone"/>
      <JFXButton fx:id="createButt" layoutX="260.0" layoutY="527.0" text="Create">
         <font>
            <Font name="System Bold" size="20.0"/>
         </font>
      </JFXButton>
   </Pane>
</AnchorPane>
