<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>


<AnchorPane prefHeight="593.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codefathers.cfkclient.controllers.CreateSupport">
    <Pane prefHeight="593.0" prefWidth="593.0">
        <stylesheets>
            <URL value="@../CSSs/button.css"/>
            <URL value="@../CSSs/gradient.css"/>
        </stylesheets>
        <styleClass>
            <String fx:value="roudPaneLeft"/>
            <String fx:value="Green"/>
        </styleClass>
        <JFXButton fx:id="back" contentDisplay="CENTER" ellipsisString="" focusTraversable="false" layoutX="14.0"
                   layoutY="14.0" prefHeight="11.0" prefWidth="42.0" styleClass="mainPageMenuButtons" text=" ">
            <graphic>
                <FontIcon iconColor="WHITE" iconLiteral="fa-arrow-circle-left" iconSize="30"/>
            </graphic>
            <ripplerFill>
                <RadialGradient centerX="0.4822222391764323" centerY="0.5" cycleMethod="REFLECT" focusAngle="-31.35"
                                radius="0.7897196172553802">
                    <stops>
                        <Stop color="#ccff00"/>
                        <Stop color="#32c2aa" offset="0.49794520547945204"/>
                        <Stop color="#ff4949" offset="1.0"/>
                    </stops>
                </RadialGradient>
            </ripplerFill>
        </JFXButton>
        <ImageView fitHeight="552.0" fitWidth="557.0" layoutX="21.0" layoutY="21.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../Images/support.png"/>
        </ImageView>
    </Pane>
    <Pane layoutX="594.0" prefHeight="593.0" prefWidth="367.0" style="-fx-background-color: white;"
          styleClass="roudPaneRight" stylesheets="@../CSSs/button.css">
        <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefWidth="340.0" spacing="27.0"
              style="-fx-background-color: white;">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <JFXTextField fx:id="username" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                          prefWidth="270.0" promptText="Username">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXTextField>
            <JFXPasswordField fx:id="password" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                              prefWidth="270.0" promptText="Password">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXPasswordField>
            <JFXPasswordField fx:id="rePassword" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                              prefWidth="270.0" promptText="Repeat Password">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXPasswordField>
            <JFXTextField fx:id="firstName" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                          prefWidth="270.0" promptText="First Name">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXTextField>
            <JFXTextField fx:id="lastName" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                          prefWidth="270.0" promptText="Last Name">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXTextField>
            <JFXTextField fx:id="email" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                          prefWidth="270.0" promptText="Email">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXTextField>
            <JFXTextField fx:id="phone" focusColor="#405aa8" labelFloat="true" prefHeight="45.0"
                          prefWidth="270.0" promptText="Phone Number">
                <font>
                    <Font size="15.0"/>
                </font>
            </JFXTextField>
        </VBox>
        <JFXButton fx:id="submit" layoutX="168.0" layoutY="536.0" prefHeight="45.0" prefWidth="186.0"
                   ripplerFill="#fd7272c7" text="Create Support">
            <font>
                <Font name="System Bold" size="21.0"/>
            </font>
        </JFXButton>
    </Pane>
</AnchorPane>
